body{
    background: #F5FAFF;
}
.banner{
    height: 3rem;
    background: url('../image/Group2@3x.png');
    background-size: 100% 100%;
}
.banner .ipt_div{
    position: relative;
    width: 100vw;
}
.banner .ipt_div #search{
    width: 80%;
    margin: .74rem 10%;
    border: none;
    height: .72rem;
    border-radius: .36rem;
    font-size: .28rem;
    text-indent: 40%;
}
.banner .ipt_div .search_img{
    width: .32rem;
    height: .32rem;
    position: absolute;
    top: .94rem;
    left: 36%;
    background: url('../image/Fill11@3x.png');
    background-size: 100% 100%;
}
input::-webkit-input-placeholder {
  color: #B5BCC6;
}
.list{
   width: 100vw;
   padding: .3rem;
}
.list .list_li{
    width: 100%;
    height: 3rem;
    background: #fff;
    margin-bottom: .3rem;
    box-shadow:0px 1px 2px 0px rgba(5, 46, 71, 0.16);
    padding: 0 .32rem;
}
.list .list_li .list_top{
    height: 2.4rem;
}
.list .list_li .list_top .list_left{
    width: 2.92rem;
}
.list .list_li .list_top .list_left img {
    width: 2.72rem;
    height: 1.8rem;
    margin-top: .3rem;
}
.list .list_li .list_right{
    font-size: .32rem;
    color: #052E47;
    margin-top: .22rem;
    line-height: .44rem;
    font-weight: bold;
}
.list .list_li .list_foot{
    background:#F9FCFF;
    height: .6rem;
    line-height: .6rem;
    font-size: .24rem;
}
.list_foot .eye_icon,.list_foot .share_icon{
    width: .32rem;
    height: .22rem;
}
.list_foot .f6_12{
    font-weight: bold;
    color: #666;
    margin:0 .08rem;
}
.list_foot .f9_12{
    color: #999;
    margin:0 .08rem;
}
.list_foot .eye_icon{
    background: url('../image/view@3x.png');
    background-size: 100% 100%;
}
.list_foot .share_icon{
    background: url('../image/Fill1@3x.png');
    background-size: 100% 100%;
}
.list_foot .yue,.list_foot .zhuan{
    align-items: center;
}
.list_foot .yue{
    width: 38%;
}
.list_foot .zhuan{
    width: 35%;
}
.list .time{
    flex: 1;
    color: #999;
    text-align: right;
}
/* common */
.list .list_li,.list {
    box-sizing: border-box;
}
.list_top,.list_foot ,.list_foot .yue,.list_foot .zhuan{
    display: flex;
}
.list_top .list_left,
.list_foot .yue,
.list_foot .zhuan,
.list_foot .eye_icon,
.list_foot .share_icon {
    flex-shrink: 0;
}